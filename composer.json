{
    "name": "mindedge/laravel-nuxt-php",
    "description": "A package to route all web requests to the nuxt build folder",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "nuxt",
        "vue",
        "laravel",
        "ssr",
        "spa"
    ],
    "homepage": "https://github.com/mindedge/laravel-nuxt-php",
    "authors": [
        {
            "name": "Mike McGrath",
            "email": "mmcgrath@mindedge.com"
        },
        {
            "name": "Mindedge IT",
            "email": "tech@mindedge.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.*|3.7.*|3.8.*|"
    },
    "autoload": {
        "psr-4": {
            "Mindedge\\LaravelNuxtPHP\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mindedge\\LaravelNuxtPHP\\LaravelNuxtPHPServiceProvider"
            ]
        }
    }
}
